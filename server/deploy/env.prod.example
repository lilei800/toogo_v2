# Copy to `.env` (in `server/`) and fill secrets before `docker compose up`.

TZ=Asia/Shanghai

# ===== PostgreSQL =====
PG_DB=hotgo
PG_USER=hotgo_user
PG_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# ===== Redis =====
REDIS_PASSWORD=CHANGE_ME_STRONG_PASSWORD

# ===== App secrets (MUST change) =====
TOKEN_SECRET_KEY=CHANGE_ME_RANDOM_64CHARS
TCP_CRON_SECRET_KEY=CHANGE_ME_OPTIONAL
TCP_AUTH_SECRET_KEY=CHANGE_ME_OPTIONAL

# ===== Optional proxy for exchange APIs =====
PROXY_ENABLED=false
PROXY_TYPE=socks5
PROXY_HOST=127.0.0.1
PROXY_PORT=10808

# ===== Runtime =====
GOMAXPROCS=2

