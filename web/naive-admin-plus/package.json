{
  "name": "naive-admin-plus",
  "version": "3.9.4",
  "author": {
    "name": "Ahjung",
    "email": "735878602@qq.com",
    "url": "https://github.com/jekip"
  },
  "private": true,
  "scripts": {
    "bootstrap": "pnpm install",
    "serve": "pnpm run dev",
    "dev": "vite",
    "build": "cross-env NODE_ENV=production vite build && esno ./build/script/postBuild.ts",
    "build:test": "cross-env vite build --mode test && esno ./build/script/postBuild.ts",
    "build:no-cache": "pnpm clean:cache && pnpm run build",
    "report": "cross-env REPORT=true pnpm run build",
    "type:check": "vue-tsc --noEmit --skipLibCheck",
    "preview": "pnpm run build && vite preview",
    "preview:dist": "vite preview",
    "clean:cache": "rimraf node_modules/.cache/ && rimraf node_modules/.vite",
    "clean:lib": "rimraf node_modules",
    "lint:eslint": "eslint \"{src,mock}/**/*.{vue,ts,tsx}\" --fix",
    "lint:prettier": "prettier --write --loglevel warn \"src/**/*.{js,json,tsx,css,less,scss,vue,html,md}\"",
    "lint:stylelint": "stylelint --fix \"**/*.{vue,less,postcss,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:pretty": "pretty-quick --staged",
    "reinstall": "rimraf pnpm-lock.yaml && rimraf package.lock.json && rimraf node_modules && pnpm run bootstrap"
  },
  "dependencies": {
    "@alova/adapter-axios": "^2.0.16",
    "@alova/mock": "^2.0.17",
    "@vicons/antd": "^0.10.0",
    "@vicons/ionicons5": "^0.10.0",
    "@vueup/vue-quill": "1.0.0-beta.8",
    "@vueuse/core": "^10.11.1",
    "alova": "^3.3.4",
    "axios": "^1.11.0",
    "cropperjs": "^1.6.2",
    "date-fns": "^2.30.0",
    "dayjs": "^1.11.13",
    "echarts": "^5.6.0",
    "element-resize-detector": "^1.2.4",
    "lodash-es": "^4.17.21",
    "mockjs": "^1.1.0",
    "naive-ui": "^2.42.0",
    "perfect-scrollbar": "^1.5.6",
    "pinia": "^2.3.1",
    "pinia-plugin-persistedstate": "^2.4.0",
    "print-js": "^1.6.0",
    "qrcode": "^1.5.4",
    "qs": "^6.14.0",
    "vue": "^3.5.18",
    "vue-i18n": "9.2.2",
    "vue-router": "^4.5.1",
    "vue-types": "^4.2.1",
    "vue-virtual-scroll-list": "^2.3.5",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@types/element-resize-detector": "^1.1.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.19.9",
    "@types/qrcode": "^1.5.5",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "@vue/compiler-sfc": "^3.5.18",
    "@vue/eslint-config-typescript": "^7.0.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "core-js": "^3.44.0",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.10.2",
    "eslint-plugin-prettier": "^4.2.5",
    "eslint-plugin-vue": "^7.20.0",
    "esno": "^4.8.0",
    "fs-extra": "^10.1.0",
    "less": "^4.4.0",
    "less-loader": "^9.1.0",
    "lint-staged": "^11.2.6",
    "picocolors": "^1.1.1",
    "postcss": "^8.5.6",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.3.1",
    "rimraf": "^3.0.2",
    "rollup-plugin-visualizer": "^5.14.0",
    "stylelint": "^16.22.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-order": "^6.0.4",
    "tailwindcss": "^3.4.17",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^5.4.19",
    "vite-plugin-compression": "^0.3.6",
    "vite-plugin-html": "^3.2.2",
    "vite-plugin-style-import": "^1.4.1",
    "vite-plugin-vue-setup-extend": "^0.4.0",
    "vue-eslint-parser": "^7.11.0",
    "vue-tsc": "^0.3.0",
    "vuedraggable": "^4.1.0"
  },
  "lint-staged": {
    "*.{vue,js,ts,tsx}": "eslint --fix"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "keywords": [
    "vue",
    "naive-ui",
    "naive-ui-admin",
    "vue3",
    "ts",
    "tsx",
    "admin",
    "typescript"
  ],
  "engines": {
    "node": ">=21"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "rollup",
        "webpack"
      ]
    }
  }
}
